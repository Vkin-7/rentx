{
  "name": "typescript",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "build/server.js",
  "scripts": {
    "dev": "tsx watch --inspect src/server.ts",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files",
    "test": "NODE_ENV=development jest --runInBand --detectOpenHandles",
    "coverage": "vitest run --coverage",
    "typeorm": "tsx ./node_modules/typeorm/cli",
    "typeorm-migrate": "tsx ./node_modules/typeorm/cli migration:run -d src/infra/data/database/data-source",
    "typeorm-migrate-revert": "tsx ./node_modules/typeorm/cli migration:revert -d src/infra/data/database/data-source",
    "start:build": "node build/server.js",
    "docker-up": "docker-compose up",
    "docker-up:background": "docker-compose up -D",
    "docker-start": "docker-compose start",
    "docker-stop": "docker-compose stop",
    "seed": "tsx ./node_modules/typeorm-extension/dist/cli/index.js seed -d ./src/infra/data/database/data-source.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.335.0",
    "@aws-sdk/client-ses": "^3.342.0",
    "@types/jest": "^29.5.0",
    "@types/swagger-ui-express": "^4.1.3",
    "class-transformer": "^0.5.1",
    "csv-parse": "^5.3.3",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "handlebars": "^4.7.7",
    "jsonwebtoken": "^9.0.0",
    "mime": "^3.0.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.1",
    "pg": "^8.9.0",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.6.0",
    "tsyringe": "^4.7.0",
    "typeorm": "^0.3.12",
    "typeorm-extension": "^2.6.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.5",
    "@babel/core": "^7.22.5",
    "@babel/node": "^7.22.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@types/express": "^4.17.16",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/mime": "^3.0.1",
    "@types/multer": "^1.4.7",
    "@types/nodemailer": "^6.4.7",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "^8.32.0",
    "eslint-plugin-typeorm": "^0.0.19",
    "jest": "^29.5.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsx": "^3.12.2",
    "typescript": "^4.9.4"
  }
}
